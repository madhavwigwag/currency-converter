{
  "name": "How much is it?",
  "version": "1.0",
  "description": "I'll convert the highlighted currency to INR",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "all_frames": true,
  "default_popup": "popup.html",
  "matches": ["http://*/*"],
  "permissions": ["http://*/*", "https://*/*", "contextMenus", "tabs"]
}
